{
  "name": "actions-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write '**/*.{ts,js}'",
    "format:check": "prettier --check '**/*.{ts,js}'",
    "lint": "eslint --no-error-on-unmatched-pattern --fix src/**/*.{ts,js}",
    "lint:check": "eslint --no-error-on-unmatched-pattern src/**/*.{ts,js}",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-turbo": "^1.9.9",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-unused-imports": "^2.0.0",
    "happy-dom": "^10.3.2",
    "prettier": "^3.0.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.3"
  },
  "main": "src/main.ts",
  "repository": "git@github.com:sharathkumar22/actions-demo.git",
  "author": "sharathkumar22>",
